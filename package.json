  },
  "author": {
    "name": "Joseph E. Sutton",
    "email": "joe@jstty.com",
    "url": "http://www.jstty.com"
  },
  "maintainers": [
    {
      "name": "jstty",
      "email": "joe@jstty.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.17.2",
    "morgan": "~1.8.2",
    "request": "~2.81.0",
    "stumpy": "~0.7.1",
    "transfuser": "~0.2.x",
    "when": "~3.7.7",
    "yanpm": "~1.0.x"
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-3": "~6.24.1",
    "babel-register": "~6.24.1",
    "babel-runtime": "~6.25.0",
    "chai": "~4.1.0",
    "cheerio": "~1.0.0-rc.2",
    "coveralls": "~2.13.1",
    "eslint": "~4.3.0",
    "eslint-config-standard": "~10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "~3.0.1",
    "freeport": "~1.0.5",
    "is-es6": "~1.0.0",
    "istanbul": "~0.4.4",
    "mocha": "~3.5.0",
    "shelljs": "~0.7.5",
    "supertest": "~3.0.0"
  },
  "scripts": {
    "lint": "npm run clean && ./node_modules/.bin/eslint lib examples test",
    "lint:fix": "npm run clean && ./node_modules/.bin/eslint lib examples test --fix",
    "compile": "./node_modules/.bin/babel lib --out-dir legacy",
    "clean": "find examples -name 'node_modules' -print0 | xargs -0 rm -rf",
    "prepublish": "npm run-script compile",
    "pretest": "npm run-script compile",
    "test": "NODE_ENV=production && istanbul --config=test/.istanbul.yml cover _mocha -- --prof -t 5000 -b -R spec test/tests.js",
    "coveralls": "cat ./test/.coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
